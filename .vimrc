filetype indent on
filetype on 
filetype plugin on

set autoindent	
set cursorline
set expandtab	
set hlsearch	
set ignorecase	
set incsearch	
set linebreak	
set nocompatible
set nowrap
set number	
set ruler	 
set shiftwidth=4
set showcmd
set showmatch	
set showmode
set smartcase	
set smartindent	
set smarttab	
set softtabstop=4	
set wildmenu
set wildmode=list:longest

syntax on
set background=dark

let g:paredit_leader = '\'
let g:gruvbox_contrast_dark = 'hard'

:colorscheme gruvbox 

